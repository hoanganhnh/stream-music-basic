{
  "name": "stream-music-basic",
  "version": "1.0.0",
  "description": "stream music with nodejs",
  "main": "src/server.js",
  "scripts": {
    "start": "nodemon --experimental-modules --es-module-specifier-resolution=node server.js",
    "build": "node --experimental-modules --es-module-specifier-resolution=node server.js",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.ts,.jsx,.tsx",
    "format": "prettier --write \"./src\""
  },
  "keywords": [
    "stream",
    "node"
  ],
  "author": "hoanganhnh",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2"
  },
  "type": "module"
}
